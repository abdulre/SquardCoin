<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="SquardCoin" location="startMenu"/>
	<shortcut
		name="SquardCoin Server"
		target="$INSTALL_PATH\SquardCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start SquardCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="SquardCoin Wallet"
		target="http://localhost:4876/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch SquardCoin wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>